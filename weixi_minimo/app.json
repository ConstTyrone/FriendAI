{
  "darkmode": true,
  "pages": [
    "pages/splash/splash",
    "pages/settings/settings",
    "pages/contact-list/contact-list",
    "pages/contact-detail/contact-detail",
    "pages/contact-form/contact-form",
    "pages/bind-account/bind-account",
    "pages/webview/webview",
    "pages/intent-manager/intent-manager",
    "pages/matches/matches",
    "pages/relationship-list/relationship-list",
    "pages/relationship-detail/relationship-detail",
    "pages/relationship-graph/relationship-graph"
  ],
  "themeLocation": "theme.json",
  "usingComponents": {
    "custom-tabbar": "/components/custom-tabbar/index",
    "profile-editor": "/components/profile-editor/index",
    "relationship-tip": "/components/relationship-tip/index",
    "relationship-card": "/components/relationship-card/index",
    "relationship-graph": "/components/relationship-graph/relationship-graph",
    "t-search": "tdesign-miniprogram/search/search",
    "t-cell": "tdesign-miniprogram/cell/cell",
    "t-cell-group": "tdesign-miniprogram/cell-group/cell-group",
    "t-icon": "tdesign-miniprogram/icon/icon",
    "t-button": "tdesign-miniprogram/button/button",
    "t-input": "tdesign-miniprogram/input/input",
    "t-textarea": "tdesign-miniprogram/textarea/textarea",
    "t-tag": "tdesign-miniprogram/tag/tag",
    "t-avatar": "tdesign-miniprogram/avatar/avatar",
    "t-empty": "tdesign-miniprogram/empty/empty",
    "t-loading": "tdesign-miniprogram/loading/loading",
    "t-navbar": "tdesign-miniprogram/navbar/navbar",
    "t-tabs": "tdesign-miniprogram/tabs/tabs",
    "t-tab-panel": "tdesign-miniprogram/tab-panel/tab-panel",
    "t-dialog": "tdesign-miniprogram/dialog/dialog",
    "t-toast": "tdesign-miniprogram/toast/toast",
    "t-fab": "tdesign-miniprogram/fab/fab"
  },
  "window": {
    "navigationBarTitleText": "社交关系",
    "navigationBarBackgroundColor": "#ffffff",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#f7f8fa",
    "backgroundTextStyle": "light"
  },
  "resolveAlias": {
    "@behaviors/*": "behaviors/*"
  },
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "permission": {
    "scope.addPhoneContact": {
      "desc": "用于导入手机通讯录联系人到应用中"
    }
  },
  "requiredBackgroundModes": [],
  "rendererOptions": {
    "skyline": {
      "disableABTest": true,
      "defaultDisplayBlock": true,
      "defaultContentBox": true,
      "sdkVersionBegin": "3.4.3",
      "sdkVersionEnd": "15.255.255"
    }
  }
}
